<MudPaper Width="100%">
    <MudContainer MaxWidth="MaxWidth.Large">
        <MudGrid Justify="Justify.SpaceEvenly" Spacing="6">
            <MudItem lg="3" md="3" sm="6" xs="12" Class="mudItem">
                <MudLink Href="formulas" Underline="Underline.None">
                    <MudCard Style="border: 1px solid #C8A86E">
                        <MudCardMedia Image="images/1.jpg" Height="200"/>
                        <MudCardContent>
                            <MudText Color="Color.Primary" Style="text-align: center" Typo="Typo.h5">
                                Basic <MudIconButton Style="padding: 0;color: #9D9D9D" Icon="@Icons.Material.Filled.HelpOutline" aria-label="help"></MudIconButton>
                            </MudText>
                        </MudCardContent>
                    </MudCard>
                </MudLink>
            </MudItem>


            <MudItem lg="3" md="3" sm="6" xs="12" Class="mudItem">
                <MudLink Href="formulas" Underline="Underline.None">
                    <MudCard Style="border: 1px solid #C8A86E">
                        <MudCardMedia Image="images/4.jpg" Height="200"/>
                        <MudCardContent>
                            <MudText Color="Color.Primary" Style="text-align: center" Typo="Typo.h5">
                                Gevorderd <MudIconButton Style="padding: 0;color: #9D9D9D" Icon="@Icons.Material.Filled.HelpOutline" aria-label="help"></MudIconButton>
                            </MudText>
                        </MudCardContent>
                    </MudCard>
                </MudLink>
            </MudItem>


            <MudItem lg="3" md="3" sm="6" xs="12" Class="mudItem">
                <MudLink Href="formulas" Underline="Underline.None">
                    <MudCard Style="border: 1px solid #C8A86E">
                        <MudCardMedia Image="images/5.jpg" Height="200"/>
                        <MudCardContent >
                            <MudText Color="Color.Primary" Style="text-align: center" Typo="Typo.h5">
                                All In <MudIconButton Style="padding: 0; color: #9D9D9D" Icon="@Icons.Material.Filled.HelpOutline" aria-label="help"></MudIconButton>
                            </MudText>
                        </MudCardContent>
                    </MudCard>
                </MudLink>
            </MudItem>


            <MudItem lg="3" md="3" sm="6" xs="12" Class="mudItem">
                <MudLink Href="#" Underline="Underline.None">
                    <MudCard Style="border: 1px solid #C8A86E">
                        <MudCardMedia Image="images/7.jpg" Height="200"/>
                        <MudCardContent>
                            <MudText Color="Color.Primary" Style="text-align: center" Typo="Typo.h5">Extra Materiaal</MudText>
                        </MudCardContent>
                    </MudCard>
                </MudLink>
            </MudItem>
        </MudGrid>
    </MudContainer>
</MudPaper>